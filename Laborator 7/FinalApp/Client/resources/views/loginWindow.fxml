<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="303.0" prefWidth="243.0" style="-fx-background-color: #e4ede9;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scheduleMaker.client.gui.ControllerLoginWindow">
    <children>
        <VBox alignment="TOP_CENTER" prefHeight="287.0" prefWidth="243.0">
