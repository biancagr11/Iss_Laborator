                        <Font name="Arial Black" size="12.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="13.0" fitWidth="13.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/close.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <padding>
                  <Insets right="10.0" />
               </padding>
            </HBox>
                <Pane prefHeight="57.0" prefWidth="243.0" style="-fx-background-color: #36a897;">
                    <children>
                        <Label layoutX="33.0" layoutY="16.0" prefHeight="27.0" prefWidth="189.0" text="Schedule maker" textFill="WHITE">
                            <font>
                                <Font name="Bookman Old Style Bold" size="22.0" />
                            </font>
                        </Label>
                    </children>
                </Pane>
                <HBox prefHeight="124.0" prefWidth="243.0">
                    <children>
                        <VBox alignment="CENTER_RIGHT" prefHeight="400.0" prefWidth="87.0" spacing="25.0">
                            <children>
                                <Label text="Username:" textFill="#36a897">
                                    <font>
                                        <Font size="14.0" />
                                    </font>
                                </Label>
                                <Label text="Password:" textFill="#36a897">
                                    <font>
                                        <Font name="Bookman Old Style" size="14.0" />
                                    </font>
                                </Label>
                            </children>
                            <HBox.margin>
                                <Insets right="30.0" />
                            </HBox.margin>
                        </VBox>
                        <VBox alignment="CENTER_LEFT" prefHeight="318.0" prefWidth="112.0" spacing="20.0">
                            <children>
                                <TextField fx:id="usernameField" prefHeight="25.0" prefWidth="133.0" />
                                <PasswordField fx:id="passwordField" prefHeight="15.0" prefWidth="112.0" />
                            </children>
                            <HBox.margin>
                                <Insets top="10.0" />
                            </HBox.margin>
                        </VBox>
                    </children>
                </HBox>
                <Button mnemonicParsing="false" onAction="#handleSingin" prefHeight="26.0" prefWidth="155.0" stylesheets="@../css/style.css" text="Sign in" textFill="WHITE">
