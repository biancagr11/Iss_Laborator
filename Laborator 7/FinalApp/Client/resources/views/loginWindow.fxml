                        <Font name="Arial Black" size="13.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/resize.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="closeButton" mnemonicParsing="false" style="-fx-background-color: #e4ede9;" textFill="#36a897">
