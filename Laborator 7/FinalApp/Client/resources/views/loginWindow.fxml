                  <Button fx:id="minusButton" mnemonicParsing="false" style="-fx-background-color: #e4ede9;" textFill="#36a897">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/minus_111123.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="circleButton" mnemonicParsing="false" style="-fx-background-color: #e4ede9;" textFill="#36a897">
